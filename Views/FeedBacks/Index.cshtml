@model IEnumerable<OneStopMiniProject.Models.FeedBack>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DoctorFeedBack)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HospitalFeedBack)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorFeedBack)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HospitalFeedBack)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FeedBackID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FeedBackID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FeedBackID })
        </td>
    </tr>
}

</table>
